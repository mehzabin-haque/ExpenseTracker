        <DatePicker fx:id="myDatePicker" layoutX="247.0" layoutY="26.0" onAction="#getDate" promptText="Enter a date" />
      <Button layoutX="14.0" layoutY="538.0" mnemonicParsing="false" onAction="#switchToScene4" styleClass="backgroundColor" stylesheets="@styleSheets.css" text="Go back to previous " textFill="WHITE">
         <font>
            <Font name="Algerian" size="12.0" />
         </font>
      </Button>
